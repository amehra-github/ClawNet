{
  "manifest_version": 2,

  "name": "Zoocchini",
  "description": "An effort to reduce Wildlife Trafficking",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab"
   ],
  "background": {
  "scripts": ["background.js"]
  }
}